# Database Configuration
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/onelastai_db
SYNC_DATABASE_URL=postgresql://postgres:password@localhost:5432/onelastai_db

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Keycloak Authentication
KEYCLOAK_URL=http://localhost:8080
KEYCLOAK_REALM=onelastai
KEYCLOAK_CLIENT_ID=onelastai-app
KEYCLOAK_CLIENT_SECRET=your-keycloak-client-secret

# Payment Providers
# Stripe
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# PayPal
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_RETURN_URL=http://localhost:3000/api/payment/success
PAYPAL_CANCEL_URL=http://localhost:3000/api/payment/cancel

# Lemon Squeezy
LEMON_SQUEEZY_API_KEY=your_lemon_squeezy_api_key
LEMON_SQUEEZY_STORE_ID=your_store_id

# Ollama AI Models
OLLAMA_HOST=http://localhost:11434

# Security
SECRET_KEY=your-super-secret-jwt-key
ALGORITHM=HS256

# Environment
DEBUG=True
ENVIRONMENT=development
