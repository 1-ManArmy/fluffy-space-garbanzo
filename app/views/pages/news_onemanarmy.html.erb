  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    body {
      font-family: 'Segoe UI', 'Roboto', 'Arial', sans-serif;
      background: linear-gradient(135deg, #0a0a1a 0%, #1a1a2a 50%, #2a1a3a 100%);
      color: #ffffff;
      min-height: 100vh;
      overflow-x: hidden;
    }

    .hero-section {
      padding: 4rem 2rem 3rem;
      text-align: center;
      background: radial-gradient(ellipse at center, rgba(255, 0, 100, 0.15) 0%, rgba(0, 0, 0, 0.3) 70%);
      border-bottom: 2px solid #ff0064;
    }

    .hero-title {
      font-size: 3.5rem;
      font-weight: 700;
      margin-bottom: 1rem;
      text-shadow: 0 0 20px #ff0064;
      background: linear-gradient(45deg, #ff0064, #cc0051, #ff3377);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .hero-icon {
      font-size: 4rem;
      margin-bottom: 1rem;
      text-shadow: 0 0 30px #ff0064;
    }

    .hero-tagline {
      font-size: 1.5rem;
      color: #e0e0e0;
      margin-bottom: 1rem;
      font-weight: 300;
    }

    .launch-badge {
      background: linear-gradient(45deg, #ff0064, #cc0051);
      color: #fff;
      padding: 0.8rem 2rem;
      border-radius: 25px;
      font-size: 1.1rem;
      font-weight: 600;
      display: inline-block;
      margin: 0.5rem;
      animation: pulse 2s infinite;
    }

    .premium-badge {
      background: linear-gradient(45deg, #ffaa00, #ff8800);
      color: #000;
      padding: 0.8rem 2rem;
      border-radius: 25px;
      font-size: 1rem;
      font-weight: 600;
      display: inline-block;
      margin: 0.5rem;
    }

    @keyframes pulse {
      0%, 100% { transform: scale(1); }
      50% { transform: scale(1.05); }
    }

    .content-section {
      padding: 3rem 2rem;
      max-width: 1400px;
      margin: 0 auto;
    }

    .info-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
      gap: 2rem;
      margin-bottom: 3rem;
    }

    .info-card {
      background: rgba(255, 255, 255, 0.05);
      border: 1px solid rgba(255, 0, 100, 0.2);
      border-radius: 15px;
      padding: 2rem;
      text-align: center;
      transition: all 0.3s ease;
    }

    .info-card:hover {
      border-color: #ff0064;
      transform: translateY(-5px);
      box-shadow: 0 10px 25px rgba(255, 0, 100, 0.2);
    }

    .info-icon {
      font-size: 2.5rem;
      margin-bottom: 1rem;
      text-shadow: 0 0 15px #ff0064;
    }

    .info-title {
      font-size: 1.2rem;
      color: #ff0064;
      margin-bottom: 0.5rem;
      font-weight: 600;
    }

    .info-content {
      color: #e0e0e0;
      line-height: 1.5;
      font-size: 0.95rem;
    }

    .agents-section {
      background: rgba(255, 255, 255, 0.03);
      border: 1px solid rgba(255, 0, 100, 0.2);
      border-radius: 20px;
      padding: 3rem;
      margin: 3rem 0;
    }

    .section-title {
      font-size: 2.5rem;
      color: #ff0064;
      text-align: center;
      margin-bottom: 2rem;
      text-shadow: 0 0 10px #ff0064;
    }

    .agents-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
      gap: 1.5rem;
      margin-top: 2rem;
    }

    .agent-card {
      background: rgba(0, 0, 0, 0.3);
      border: 1px solid rgba(255, 0, 100, 0.1);
      border-radius: 12px;
      padding: 1.5rem;
      transition: all 0.3s ease;
      position: relative;
      overflow: hidden;
    }

    .agent-card:hover {
      border-color: #ff0064;
      background: rgba(255, 0, 100, 0.05);
      transform: translateX(5px);
    }

    .agent-number {
      color: #ffaa00;
      font-weight: 700;
      font-size: 0.9rem;
      margin-bottom: 0.5rem;
    }

    .agent-name {
      color: #ff0064;
      font-weight: 600;
      font-size: 1.1rem;
      margin-bottom: 0.8rem;
      font-family: 'Courier New', monospace;
    }

    .agent-description {
      color: #e0e0e0;
      line-height: 1.5;
      font-size: 0.9rem;
    }

    .warning-section {
      background: rgba(255, 170, 0, 0.1);
      border: 2px solid #ffaa00;
      border-radius: 15px;
      padding: 2rem;
      margin: 2rem 0;
      text-align: center;
    }

    .warning-title {
      color: #ffaa00;
      font-size: 1.5rem;
      font-weight: 700;
      margin-bottom: 1rem;
    }

    .warning-text {
      color: #e0e0e0;
      line-height: 1.6;
      font-size: 1rem;
    }

    .back-button {
      display: inline-block;
      background: linear-gradient(45deg, #666, #888);
      color: #ffffff;
      padding: 1rem 2rem;
      border-radius: 12px;
      text-decoration: none;
      font-weight: 600;
      transition: all 0.3s ease;
      margin-top: 2rem;
    }

    .back-button:hover {
      transform: translateY(-2px);
      box-shadow: 0 10px 30px rgba(136, 136, 136, 0.4);
    }

    @media (max-width: 768px) {
      .hero-title {
        font-size: 2.5rem;
      }
      
      .info-grid {
        grid-template-columns: 1fr;
      }
      
      .agents-grid {
        grid-template-columns: 1fr;
      }
      
      .agents-section {
        padding: 2rem;
      }
    }
  </style>
</head>

